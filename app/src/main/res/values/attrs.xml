<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="curvedHeaderGradientTop" format="color" />
    <attr name="curvedHeaderGradientBottom" format="color" />
    <attr name="frontSurface" format="color" />
    <attr name="backSurface" format="color" />
    <attr name="primaryHeaderText" format="color" />
    <attr name="secondaryHeaderText" format="color" />
    <attr name="primarySurfaceText" format="color" />
    <attr name="secondarySurfaceText" format="color" />
    <attr name="primaryHorizontalDivider" format="color" />
    <attr name="secondaryHorizontalDivider" format="color" />
    <attr name="selectionArrowColor" format="color" />
    <attr name="durationIcon" format="reference" />
    <attr name="endTimeIcon" format="reference" />
    <attr name="startTimeIcon" format="reference" />
    <attr name="graphXAxis" format="color" />
    <attr name="timeseries" format="color" />
    <attr name="timeseriesGap" format="color" />
    <attr name="heartHistoricalAvg" format="color" />
    <attr name="addNoteText" format="color" />
    <attr name="messageBoxBackground" format="color" />
    <attr name="primaryText" format="color" />
    <attr name="circularSeekBar" format="color" />
    <attr name="circularSeekBarCircleFill" format="color" />
    <attr name="circularSeekBarProgress" format="color" />
    <attr name="primaryIcon" format="color" />
    <attr name="secondaryIcon" format="color" />
    <attr name="pointerIcon" format="color" />
    <attr name="playScreenErrorBackground" format="color" />
    <attr name="transparentButton" format="color" />
    <attr name="buttonBackground" format="color" />
    <attr name="stopTrackingButton" format="color" />
    <attr name="lightText" format="color" />
    <attr name="hintText" format="color" />
    <attr name="primaryBottomSheet" format="color" />
    <attr name="secondaryBottomSheet" format="color" />
    <attr name="bottomSheetDivider" format="color" />
    <attr name="bottomSheetDescriptionText" format="color" />
    <attr name="awardsBackground" format="color" />
    <attr name="playerSettingBackground" format="color" />
    <attr name="playerSettingCircleButton" format="color" />
    <attr name="selectedDuration" format="color" />
    <attr name="subscriptionTextColorTitle" format="color" />
    <attr name="chevronColor" format="color" />
    <attr name="tabButtonBackgroundUnselected" format="color" />
    <attr name="tabButtonBackgroundSelected" format="color" />

    <declare-styleable name="CustomToolbar">
        <attr name="bgcolor" format="color" />
        <attr name="leftActionIcon" format="reference" />
        <attr name="rightActionIcon" format="reference" />
        <attr name="toolbar_title" format="string" />
        <attr name="right_action_type" />
    </declare-styleable>


    <declare-styleable name="CustomEditText">
        <attr name="inputType" />
        <attr name="hint" format="string" />
        <attr name="showInfo" format="boolean" />
        <attr name="bgError" format="color" />
        <attr name="textColor" format="color" />
        <attr name="underlineColor" format="color" />
    </declare-styleable>
    <declare-styleable name="LoadingButton">
        <attr name="button_text" format="string" />
        <attr name="text_color" format="color" />
        <attr name="pb_color" />

    </declare-styleable>
    <attr name="pb_color" format="enum">
        <enum name="blue" value="0" />
        <enum name="white" value="1" />
    </attr>
    <attr name="inputType" format="enum">
        <enum name="textCapWords" value="0" />
        <enum name="textPassword" value="1" />
        <enum name="textEmailAddress" value="2" />
        <enum name="text" value="3" />
    </attr>


    <declare-styleable name="CirclePageIndicator">
        <!-- Whether or not the indicators should be centered. -->
        <attr name="centered" format="boolean" />
        <!-- Color of the filled circle that represents the current page. -->
        <attr name="fillColor" format="color" />
        <!-- Color of the filled circles that represents pages. -->
        <attr name="pageColor" format="color" />
        <!-- Orientation of the indicator. -->
        <attr name="android:orientation" />
        <!-- Radius of the circles. This is also the spacing between circles. -->
        <attr name="radius" format="dimension" />
        <!-- Margin between circles. This is also the spacing between circles. -->
        <attr name="margin" format="dimension" />
        <!-- Whether or not the selected indicator snaps to the circles. -->
        <attr name="snap" format="boolean" />
        <!-- Color of the open circles. -->
        <attr name="strokeColor" format="color" />
        <!-- Width of the stroke used to draw the circles. -->
        <attr name="strokeWidth" format="dimension" />
        <!-- View background -->
        <attr name="android:background" />
    </declare-styleable>
    <declare-styleable name="ViewPagerIndicator">
        <!-- Style of the circle indicator. -->
        <attr name="vpiCirclePageIndicatorStyle" format="reference" />
        <!-- Style of the icon indicator's views. -->
        <attr name="vpiIconPageIndicatorStyle" format="reference" />
        <!-- Style of the line indicator. -->
        <attr name="vpiLinePageIndicatorStyle" format="reference" />
        <!-- Style of the title indicator. -->
        <attr name="vpiTitlePageIndicatorStyle" format="reference" />
        <!-- Style of the tab indicator's tabs. -->
        <attr name="vpiTabPageIndicatorStyle" format="reference" />
        <!-- Style of the underline indicator. -->
        <attr name="vpiUnderlinePageIndicatorStyle" format="reference" />
    </declare-styleable>


    <declare-styleable name="MuseoTextView">
        <attr name="typeface" />
    </declare-styleable>

    <declare-styleable name="MuseoButton">
        <attr name="typeface" />
    </declare-styleable>

    <attr name="typeface">

        <enum name="PROXIMA_NOVA_BOLD" value="0" />
        <enum name="PROXIMA_NOVA_SBOLD" value="1" />
        <enum name="PROXIMA_NOVA_REG" value="2" />
        <enum name="PROXIMA_NOVA_LIGHT" value="3" />
        <enum name="PROXIMA_NOVA_REG_ITALIC" value="4" />

        <enum name="MUSEO_SANS_ROUNDED_100" value="100" />
        <enum name="MUSEO_SANS_ROUNDED_300" value="101" />
        <enum name="MUSEO_SANS_ROUNDED_500" value="102" />
        <enum name="MUSEO_SANS_ROUNDED_700" value="103" />
        <enum name="MUSEO_SANS_ROUNDED_900" value="104" />
        <enum name="MUSEO_SANS_ROUNDED_1000" value="105" />

    </attr>

    <declare-styleable name="CircularSeekBar">
        <attr name="circle_style">
            <enum name="butt" value="0" />
            <enum name="round" value="1" />
            <enum name="square" value="2" />
        </attr>
        <attr name="progress" format="integer" />
        <attr name="max" format="integer" />
        <attr name="lock_enabled" format="boolean" />
        <attr name="circle_stroke_width" format="dimension" />
        <attr name="circle_fill" format="color" />
        <attr name="circle_color" format="color" />
        <attr name="circle_progress_color" format="color" />
        <attr name="enable_interaction" format="boolean" />
        <attr name="pointer_color" format="color" />
        <attr name="pointer_stroke_width" format="dimension" />
        <attr name="start_angle" format="float" />
        <attr name="end_angle" format="float" />
    </declare-styleable>

</resources>
